<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="OnRibbonLoad">
	<ribbon>
		<tabs>
			<tab id="tabRealWare" 
				 label="RealWare">
				<group id="grpConnection" 
					   label="Connection">
					<toggleButton id="btnConnectToRealWare" 
								  onAction="OnConnectToRealWareClick"
								  getImage="GetConnectToRealWareIcon" 
								  getPressed="GetConnectToRealWarePressed"
								  getLabel="GetConnectToRealWareLabel"
								  size="large"/>
				</group>
				<group id="grpAPI"
					   label="API">
					<button id="btnImportFromListBuilder"
							label="Import from ListBuilder"
							getImage="GetImportFromListBuilderImage"
							getEnabled="GetImportFromListBuilderEnabled"
							onAction="OnImportFromRealWareClick"
							getVisible="GetImportFromListBuilderVisible"
							size="large"/>
					<button id="btnBatchAccounts"
							label="Batch Accounts"
							getImage="GetBatchAccountsImage"
							getEnabled="GetBatchAccountsEnabled"
							onAction="OnBatchAccountsClick"
							size="large"/>
					<button id="btnViewScripts"
							label="View Batch Scripts"
							getImage="GetViewBatchScriptsImage"
							getEnabled="GetViewBatchScriptsEnabled"
							onAction="OnViewBatchScriptsClick"
							size="large"/>
				</group>
				<group id="grpSettings"
					   label="Settings">
					<button id="btnSettings"
							label="Settings"
							getImage="GetSettingsImage"
							onAction="OnSettingsClick"
							size="large"/>
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>
